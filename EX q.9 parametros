#include <stdio.h>

int main()
{   float a,b,c,d,e,f,x,y;
    printf("Digite os valores a,b,c,d,e,f:");
    scanf("%f%f%f%f%f%f",&a,&b,&c,&d,&e,&f);
    x= ((c*e)-(b*f))/((a*e)-(b*d));
    y= ((a*f)-(c*d))/((a*e)-(b*d));
    c= (a*x)+(b*y);
    f= (d*x)+(e*y);
    printf("\nSeu valor de x e y Ã©: %.2f\t%.2f",x,y);
    return 0;
}
